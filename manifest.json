{
    "manifest_version": 3,
    "name": "hideyoutubec",
    "description": "Chrome extension",
    "version": "1.0.0",
    "content_scripts": [{
        "matches": ["https://www.youtube.com/"],
        "js": ["./script.js"],
        "run_at": "document_start"
    }],
    "action": {
        "default_popup": "popup.html"
    }
}